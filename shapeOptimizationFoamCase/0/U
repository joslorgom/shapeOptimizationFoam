FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

velocity_0	uniform (25.75 3.62 0);
velocity	uniform (1 0 0);

internalField   $velocity;

boundaryField
{
    inf
    {
	type		fixedValue;
	value		$velocity;
    }
/*
    outlet
    {
	type		zeroGradient;
    }
*/
    walls
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}

